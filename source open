-- Ensure ScrollingFrame is correctly referenced
local tweenService = game:GetService("TweenService")
local ScrollingFrame4 = a["ScrollingFrame 4"]
if not ScrollingFrame4 then
    error("ScrollingFrame not found in 'a'!")
end

local buttons = {}

local function createExecuteScriptButton(name, scriptFunction)
    local button3 = Instance.new("TextButton")
    local buttonCorner3 = Instance.new("UICorner")


    button3.Position = UDim2.new(0.15011820197105408, 0, 0.0002540761779528111, 0)
    button3.Size = UDim2.new(0, 385, 0, 39)
    button3.BackgroundColor3 = Color3.fromRGB(6, 64, 108)
    button3.FontFace = Font.new("rbxasset://fonts/families/Roboto.json", Enum.FontWeight.Regular, Enum.FontStyle.Normal)
    button3.Text = name
    button3.TextColor3 = Color3.fromRGB(255, 255, 255)
    button3.TextSize = 17
    button3.Name = "Animation"
    button3.Parent = ScrollingFrame4
    button3.AutoButtonColor = false


    buttonCorner3.CornerRadius = UDim.new(0, 5)
    buttonCorner3.Name = "butcorner"
    buttonCorner3.Parent = button3


    local infobutton3 = Instance.new("ImageButton")
    infobutton3.Position = UDim2.new(0.02857142873108387, 0, 0.23076924681663513, 0)
    infobutton3.Size = UDim2.new(0, 20, 0, 21)
    infobutton3.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    infobutton3.BackgroundTransparency = 1
    infobutton3.Image = "http://www.roblox.com/asset/?id=6294110112"
    infobutton3.Name = "infobutton"
    infobutton3.Parent = button3

    table.insert(buttons, button3)

    button3.MouseButton1Click:Connect(function()
        a["Sound 2"]:Play()
        pcall(scriptFunction)
    end)

    button3.MouseButton1Down:Connect(function()
        local hoverTween = tweenService:Create(
            button3,
            TweenInfo.new(0.4, Enum.EasingStyle.Linear),
            {BackgroundColor3 = Color3.fromRGB(105, 158, 158)}
        )
        hoverTween:Play()
    end)

    button3.MouseButton1Up:Connect(function()
        local leaveTween = tweenService:Create(
            button3,
            TweenInfo.new(0.4, Enum.EasingStyle.Linear),
            {BackgroundColor3 = Color3.fromRGB(6, 64, 108)}
        )
        leaveTween:Play()
    end)
end

createExecuteScriptButton("Infinite Yield", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
end)

createExecuteScriptButton("Xora Safety", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/sytcal/Xora/main/EverthingIsForYourSafety"))()
end)

createExecuteScriptButton("Pluto V3", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Horizon89002/PLUT0_V3/refs/heads/main/PLUT0%20V3%20(source).lua"))()
end)

createExecuteScriptButton("System Broken", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/H20CalibreYT/SystemBroken/main/script"))()
end)

createExecuteScriptButton("Debuggers", function()
    loadstring(game:HttpGet(('https://raw.githubusercontent.com/infyiff/backup/main/audiologger.lua'),true))()
end)

createExecuteScriptButton("Audio Logger", function()
    loadstring(game:HttpGet(('https://raw.githubusercontent.com/infyiff/backup/main/audiologger.lua'), true))()
end)

