a["SearchBar"]["Position"] = UDim2.new(0, 12, 0, 21)
a["SearchBar"]["Size"] = UDim2.new(0, 396, 0, 34)
a["SearchBar"]["BackgroundColor3"] = Color3.fromRGB(0, 80, 138)
a["SearchBar"]["BackgroundTransparency"] = 0.5
a["SearchBar"]["FontFace"] = Font.new("rbxasset://fonts/families/SourceSansPro.json", Enum.FontWeight.Regular, Enum.FontStyle.Normal)
a["SearchBar"]["Text"] = ""
a["SearchBar"]["TextColor3"] = Color3.fromRGB(255, 255, 255)
a["SearchBar"]["TextSize"] = 20
a["SearchBar"]["PlaceholderText"] = "search bar"
a["SearchBar"]["Name"] = "SearchBar"
a["SearchBar"]["Parent"] = a["Animations"]

a["SearchBar"]:GetPropertyChangedSignal("Text"):Connect(function()
    local searchText = string.lower(a["SearchBar"].Text)
    for _, button in ipairs(buttons) do
        if string.find(string.lower(button.Text), searchText, 1, true) then
            button.Visible = true
        else
            button.Visible = false
        end
    end
end)


a["searchicon"]["Position"] = UDim2.new(.03799999877810478, 0, .23000000417232513, 0)
a["searchicon"]["Size"] = UDim2.new(0, 20, 0, 20)
a["searchicon"]["BackgroundColor3"] = Color3.fromRGB(255, 255, 255)
