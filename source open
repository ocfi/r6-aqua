local tweenService2 = game:GetService("TweenService")

-- Ensure ScrollingFrame is correctly referenced
local ScrollingFrame3 = a["ScrollingFrame 3"]
if not ScrollingFrame3 then
    error("ScrollingFrame not found in 'a'!")
end

-- Function to create buttons for animations
local function createSpeedButton(name, speed)
    local button2 = Instance.new("TextButton")
    local buttonCorner2 = Instance.new("UICorner")
    
    -- Apply provided properties
    button2.Size = UDim2.new(0, 385, 0, 39)
    button2.BackgroundColor3 = Color3.fromRGB(6, 64, 108)
    button2.FontFace = Font.new("rbxasset://fonts/families/Roboto.json", Enum.FontWeight.Regular, Enum.FontStyle.Normal)
    button2.Text = name
    button2.TextColor3 = Color3.fromRGB(255, 255, 255)
    button2.TextSize = 17
    button2.Name = "Animation"
    button2.Parent = ScrollingFrame3
    button2.AutoButtonColor = false

    buttonCorner2.CornerRadius = UDim.new(0, 5)
    buttonCorner2.Name = "butcorner"
    buttonCorner2.Parent = button2

    local infobutton2 = Instance.new("ImageButton")
    infobutton2.Position = UDim2.new(0.02857142873108387, 0, 0.23076924681663513, 0)
    infobutton2.Size = UDim2.new(0, 20, 0, 21)
    infobutton2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    infobutton2.BackgroundTransparency = 1
    infobutton2.Image = "http://www.roblox.com/asset/?id=6294110112"
    infobutton2.Name = "infobutton"
    infobutton2.Parent = button2

    -- Store button reference for search functionality
    table.insert(buttons, button2)

    -- Hover effect
    button2.MouseButton1Down:Connect(function()
        local hoverTween2 = tweenService2:Create(button2,
            TweenInfo.new(0.4, Enum.EasingStyle.Linear),
            {BackgroundColor3 = Color3.fromRGB(105, 158, 158)}
        )
        hoverTween2:Play()
    end)

    -- Leave effect
    button2.MouseButton1Up:Connect(function()
        local leaveTween2 = tweenService2:Create(button2,
            TweenInfo.new(0.4, Enum.EasingStyle.Linear),
            {BackgroundColor3 = Color3.fromRGB(6, 64, 108)}
        )
        leaveTween2:Play()
    end)

    -- Button click functionality
    button2.MouseButton1Click:Connect(function()
        local humanoid = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            -- Adjust speed for each active animation
            for _, animationTrack in pairs(activeAnimations) do
                if animationTrack then
                    animationTrack:AdjustSpeed(speed) -- Use the predefined speed
                end
            end
        end
    end)
end

